<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <com.baidu.mapapi.map.MapView
        android:id="@+id/mv_main_ordercenter_orderdetail_checkdetail_baidumap"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    </com.baidu.mapapi.map.MapView>
    <ImageButton
        android:id="@+id/ib_main_ordercenter_orderdetail_checkdetail_baidumap_locself"
        android:layout_width="@dimen/dimen_main_ordercenter_orderdetail_checkdetail_baidumap_gps_loc_mark_width"
