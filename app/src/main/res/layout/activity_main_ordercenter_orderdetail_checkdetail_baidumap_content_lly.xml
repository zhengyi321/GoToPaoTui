        android:background="@drawable/gps_loc"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/dimen_main_ordercenter_orderdetail_checkdetail_baidumap_gps_loc_mark_margin_right_size"
        android:layout_marginBottom="@dimen/dimen_main_ordercenter_orderdetail_checkdetail_baidumap_gps_loc_mark_margin_bottom_size"
        />
    <ImageView
        android:id="@+id/iv_main_addressmanage_add_user_centerloc"
        android:layout_width="@dimen/dimen_main_ordercenter_orderdetail_checkdetail_baidumap_loc_goto_mark_width"
        android:layout_height="@dimen/dimen_main_ordercenter_orderdetail_checkdetail_baidumap_loc_goto_mark_height"
        android:background="@drawable/courier_logo"
        android:layout_centerInParent="true"
        android:visibility="invisible"
        />
</RelativeLayout>